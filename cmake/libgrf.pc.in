prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include/libgrf

Name: libgrf
Description: A library for reading and writing GRF archives.
Version: @LIBGRF_VERSION@
Requires: zlib
Libs: -L${libdir} -lz -lgrf -DGRF_STATIC -DSPR_STATIC
Cflags: -I${includedir}
